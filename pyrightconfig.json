{
  "include": ["src", "tests"],
  "exclude": [
    "**/system_hw_test",
    "**/__pycache__",
    "**/unitree",
    "**/ubtech"
  ],
  "typeCheckingMode": "standard",
  "pythonVersion": "3.10",
  "executionEnvironments": [
    {
      "root": "src",
      "reportMissingImports": "warning",
      "reportMissingModuleSource": "warning"
    },
    {
      "root": "tests",
      "reportMissingImports": "none",
      "reportMissingModuleSource": "none"
    }
  ]
}
